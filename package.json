  "repository": {
    "type": "git",
    "url": "git@github.com:ncukondo/gdoc-parser.git"
  },
  "scripts": {
    "lint:init": "npx eslint --init",
    "lint": "npx eslint --fix src/**/*.ts",
    "test": "jest",
    "clasp-login": "npx clasp login ../config/clasprc.json --no-localhost",
    "preversion": "yarn build && node make_clasp_json.js",
    "postversion": "git push --tags && git push origin main && rimraf .clasp.json",
    "build": "yarn lint && rimraf dist && rollup -c && cpx src/appsscript.json dist && cpx js/**/*.js dist",
    "deploy": "yarn build && node make_clasp_json.js && npx clasp push && rimraf .clasp.json",
    "release": "yarn version",
    "update_env": "sh .devcontainer/sh/register-dot-env-github"
  },
  "dependencies": {},
  "devDependencies": {
    "@google/clasp": "^2.3.0",
    "@jo12bar/rollup-plugin-hoist-entry-exports-to-global-scope": "^1.0.3",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/google-apps-script": "^1.0.14",
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^5.24.0",
    "@typescript-eslint/parser": "^5.24.0",
    "cpx": "^1.5.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^26.4.2",
    "lodash.camelcase": "^4.3.0",
    "lodash.upperfirst": "^4.3.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.6.4"
  }
